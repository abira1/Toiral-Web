{
  "name": "magic-patterns-vite-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node scripts/generate-firebase-sw.js && npx vite",
    "build": "cross-env NODE_ENV=production node scripts/generate-firebase-sw.js && npx vite build",
    "build:dev": "cross-env NODE_ENV=development node scripts/generate-firebase-sw.js && npx vite build",
    "build:windows": "set NODE_ENV=production && node scripts/generate-firebase-sw.js && npx vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "preview": "npx vite preview",
    "generate-sw": "node scripts/generate-firebase-sw.js",
    "test-env": "cross-env NODE_ENV=development node scripts/test-env-vars.js",
    "test-env:prod": "cross-env NODE_ENV=production node scripts/test-env-vars.js",
    "deploy": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@types/lodash": "^4.17.16",
    "@types/react-helmet": "^6.1.11",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "firebase": "^11.7.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.441.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.26.2",
    "react-scroll": "^1.9.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "latest",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "postcss": "latest",
    "tailwindcss": "3.4.17",
    "terser": "^5.39.0",
    "typescript": "^5.5.4",
    "vite": "^6.3.2"
  }
}
